#
# BootManager Command Launcher definitions
# Eric Jeschke  2008.09.14
#
LABEL "Restart Server"
	VAL01 LIST  "Task Manager"="taskmgr0","Status"="status","Telescope Interface"="TSC0","STARS Interface"="STARSint","Frame Manager"="frames","Session Manager"="sessions","Monitor"="monitor","Instrument Simulator"="simcam","Telescope Simulator"="tsccam","STARS Simulator"="STARSsim" "Subsystem"
	CMD EXEC BOOTMANAGER restart $VAL01

LABEL "Restart Client"
	VAL01 LIST  "IntegGUI"="integgui","VGW"="vgw","QDAS"="qdas","Sky Monitor"="skymon","FITS Viewer"="fitsview","TelStat"="telstat" "Application"
	CMD EXEC BOOTMANAGER restart $VAL01

LABEL "Custom Restart"
	VAL01 INPUT 40 "vgw" "Subsystem"
	CMD EXEC BOOTMANAGER restart $VAL01

LABEL "Stop All"
	CMD EXEC BOOTMANAGER stopall

LABEL "Reset"
	CMD EXEC BOOTMANAGER reset

LABEL "To Stage"
	VAL01 INPUT 10 0 "Stage"
	CMD EXEC BOOTMANAGER tostage $VAL01

<>

#LABEL "Clear Config"
#	CMD EXEC BOOTMANAGER clearall

LABEL "Reload Config"
	CMD EXEC BOOTMANAGER reloadConfig

LABEL "Distribute Config"
	CMD EXEC BOOTMANAGER setup

#END
